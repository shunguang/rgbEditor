    <highlightSegmentLengthInSec>15</highlightSegmentLengthInSec>
    <minApplausingTimeSec>2</minApplausingTimeSec>
    <minSegmentTimeSec>5</minSegmentTimeSec>
    <fontSize>9</fontSize>
